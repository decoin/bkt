<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
	<hibernate-mapping package="com.bkt.bean">
		<class name="Orderlist" table="orderlist">
			<id name="orderlist_id" column="orderlist_id">
				<generator class="identity"></generator>
			</id>
			<property name="orderno" type="java.lang.String" column="orderno" not-null="true"></property>
			<property name="flightno" type="java.lang.String" column="flightno"></property>
			<property name="name" type="java.lang.String" column="name"></property>
			<property name="num" type="java.lang.Integer" column="num"></property>
			<property name="total" type="java.lang.Float" column="total"></property>
			<property name="orderdate" type="java.lang.String" column="orderdate"></property>
		</class>	
	</hibernate-mapping>